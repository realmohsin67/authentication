{
  "name": "nextjs-prisma-ts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postinstall": "prisma generate --no-engine",
    "start": "next start",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "next lint",
    "types:check": "tsc --noEmit",
    "validate": "npm-run-all --parallel format:check lint:check types:check",
    "db:client:generate": "prisma generate",
    "db:dev:push": "prisma db push",
    "db:migration:create": "prisma migrate dev --create-only",
    "db:dev:migration:apply": "prisma migrate dev",
    "db:prod:migration:apply": "prisma migrate deploy",
    "db:dev:reset": "prisma migrate reset",
    "db:dev:seed:data": "tsx prisma/seed-data.ts",
    "db:dev:seed:constants": "tsx prisma/seed-constants.ts",
    "db:dev:studio": "prisma studio",
    "db:connect": ". ./.env && ppg-tunnel --host $TUNNEL_HOST --port $TUNNEL_PORT",
    "db:dump:sql": ". ./.env && PGSSLMODE=disable pg_dump -h $TUNNEL_HOST -p $TUNNEL_PORT -F p -v -d postgres -f ./prisma/db-dump.ignore.sql --column-inserts"
  },
  "engines": {
    "node": "22.18.0",
    "npm": "10.9.2"
  },
  "dependencies": {
    "@prisma/client": "6.8.2",
    "@prisma/extension-accelerate": "2.0.1",
    "bcryptjs": "3.0.2",
    "next": "15.3.3",
    "qrcode": "1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "4.5.1",
    "time2fa": "1.4.2"
  },
  "devDependencies": {
    "@faker-js/faker": "9.8.0",
    "@prisma/ppg-tunnel": "0.3.0",
    "@types/node": "22.15.15",
    "@types/qrcode": "1.5.5",
    "@types/react": "19.1.3",
    "chalk": "5.4.1",
    "eslint": "9.26.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "10.1.3",
    "log-symbols": "7.0.1",
    "msw": "2.8.6",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "prisma": "6.8.2",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  }
}
